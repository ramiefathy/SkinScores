{
  "name": "skinscores",
  "version": "1.0.0",
  "description": "SkinScores clinical scoring workspace",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "test": "firebase emulators:exec --project skinscores-test --only firestore,functions,auth \"vitest run\"",
    "test:watch": "vitest",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "serve": "firebase serve",
    "deploy": "firebase deploy",
    "emulators": "firebase emulators:start",
    "seed:emulator": "node scripts/seed-emulator.cjs --project=skinscores-test",
    "clear:aggregates": "node scripts/clear-aggregates.mjs --project=skinscores-test"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@hookform/resolvers": "^3.9.0",
    "@mui/icons-material": "^6.1.4",
    "@mui/lab": "^6.0.0-beta.20",
    "@mui/material": "^6.1.5",
    "@paper-design/shaders": "^0.0.52",
    "@paper-design/shaders-react": "^0.0.52",
    "@tanstack/react-query": "^5.59.15",
    "date-fns": "^3.6.0",
    "firebase": "^11.0.1",
    "firebase-admin": "^12.6.0",
    "fuse.js": "^7.1.0",
    "lucide-react": "^0.544.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.0",
    "react-router-dom": "^6.26.1",
    "zod": "^3.23.8",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@firebase/rules-unit-testing": "^4.0.0",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.3.7",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "jsdom": "^25.0.0",
    "minimist": "^1.2.8",
    "playwright": "^1.55.0",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "tslib": "^2.7.0",
    "typescript": "^5.5.4",
    "vite": "^5.4.7",
    "vitest": "^2.0.5"
  },
  "type": "module"
}
